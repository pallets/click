msgstr "Невалидна стойност за {param_hint}: {message}"

msgid  "Missing argument."
msgstr "Липсващ аргумент."

msgid  "Missing option."
msgstr "Липсваща опция."

msgid  "Missing parameter."
msgstr "Липсващ параметър."

